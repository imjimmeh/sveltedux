{
  "name": "sveltedux",
  "version": "0.0.3",
  "author": {
    "name": "Jim Washbrook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imjimmeh/sveltedux"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "svelte": "./dist/core.js"
    },
    "./async": {
      "types": "./dist/async-toolkit.d.ts",
      "svelte": "./dist/async-toolkit.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "svelte": "./dist/api.js"
    },
    "./persistence": {
      "types": "./dist/persistence.d.ts",
      "svelte": "./dist/persistence.js"
    },
    "./utilities": {
      "types": "./dist/utilities.d.ts",
      "svelte": "./dist/utilities.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@vitest/browser": "^3.2.3",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "playwright": "^1.53.0",
    "publint": "^0.3.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.3",
    "vitest-browser-svelte": "^0.1.0"
  },
  "keywords": [
    "svelte",
    "redux",
    "sveltedux",
    "state management"
  ],
  "license": "MIT",
  "dependencies": {
    "immer": "^10.1.1"
  }
}
